import pandas as pd
from keplergl import KeplerGl

# Load Data
workers = pd.read_csv('../data/workers_geo.csv', parse_dates=['last_active'])
gigs = pd.read_csv('../data/gigs_geo.csv', parse_dates=['start_time'])
no_show = pd.read_csv('../data/no_show_history.csv')

# Initialize Kepler Map
map_ = KeplerGl(height=600)

# Add datasets
map_.add_data(data=workers, name="Workers")
map_.add_data(data=gigs, name="Gigs")
map_.add_data(data=no_show, name="No Show History")

# Save configuration (optional)
# map_.save_to_html(file_name="../exports/heatmap_output.html")

map_
